		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int[] arr = new int[100];
		int pos = 0;
		for(int i = 2; n > 0; ++i)
		{
			while(n % i == 0 && n > 0)
			{
				System.out.println(i);
				arr[pos++] = i;
				n /= i;
			}
		}
		System.out.print(arr[0]);
		for(int i = 1; i < pos; ++i)
			System.out.print('*' + arr[i]);
		System.out.println();
	}
}