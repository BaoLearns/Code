//package hello1;	
import java.util.*;
public class Main {
	public static boolean Is(int n)
	{
		int[] arr = new int[10];
		int pos = 0;
		while(n > 0)
		{
			arr[pos++] = n % 10;
			n /= 10;
		}
		for(int i = 0; i < pos / 2; ++i)
			if(arr[i] != arr[pos - i - 1])
				return false;
		return true;
		
	}
	public static void main(String[] args){
		int n;
		int[] arr = new int[100];
		Scanner in = new Scanner(System.in);
		n = in.nextInt();
		for(int i = 0; i < n; ++i)
			arr[i] = in.nextInt();
		for(int i = 0; i < n; ++i)
		{
			int l = 0, r = 0;
			for(int j = arr[i]; j >= 0; --j)
				if(Is(j))
				{
					l = j;
					break;
					
				}
			for(int j = arr[i] + 1;;++j)
				if(Is(j))
				{
					r = j;
					break;
					
				}

			System.out.println(Math.abs(arr[i] - l) > Math.abs(arr[i] - r)? Math.abs(arr[i] - l): Math.abs(arr[i] - r));
		}
	}
}